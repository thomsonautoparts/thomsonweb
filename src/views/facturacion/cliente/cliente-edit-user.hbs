    <main>

        <div class="container-fluid mt-4">
            <div class="row">
                <div class="col-md">

                    <div class="container-fluid mt-2">
                        {{#each errors}}

                        <div class="alert alert-danger alert-dismissible fade show" role="alert">
                            {{text}}
                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                        </div>

                        {{/each}}
                        {{#each ok}}

                        <div class="alert alert-success alert-dismissible fade show" role="alert">
                            {{text}}
                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                        </div>

                        {{/each}}
                        <div class="card mt-2">
                                <form action="/cliente-edited-user/{{usuario._id}}" method="POST">
                            <div class="card-header">
                                <h4 class="text-dark">Datos de usuario</h4>
                            </div>
                            <div class="card-body">
                                    <div class="row">
                                    <div class="col-sm-6">
                                        <lable class="text-dark">Correo electronico</lable>
                                        <input type="email" name="email" class="form-control" value="{{usuario.email}}">
                                    </div>
                                    <div class="col-sm-6">
                                        <lable class="text-dark">Confirmar correo electronico</lable>
                                        <input type="email" name="emailconfirm" class="form-control" value="{{usuario.email}}">
                                    </div>
                                    <div class="col-sm-6">
                                        <label class="text-dark">Contraseña nueva</label>
                                        <input type="password" name="password" class="form-control" value="{{usuario.password}}" placeholder="Introducir nueva contraseña">
                                    </div>
                                    <div class="col-sm-6">
                                        <label class="text-dark">Confirmar contraseña nueva</label>
                                        <input type="password" name="passwordconfirm" class="form-control" value="{{usuario.password}}" placeholder="Confirmar nueva contraseña">
                                    </div>
                                    </div>
                            </div>
                        </div>
                        <div class="mt-2 mb-3 text-end">
                            <input type="submit" value="Actualizar cliente" class="btn btn-outline-success">
                        </div>
                        </form>
                    </div>

                </div>
            </div>
        </div>
