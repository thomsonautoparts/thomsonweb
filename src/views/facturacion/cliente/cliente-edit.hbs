    <script src="/js/bootstrap.js"></script>
        <script>
            const d = document
            const valorPrecio = d.getElementById("valorPrecio")
            const TipoPrecio = d.getElementById("TipoPrecio")
            const valorVendedor = d.getElementById("ValorVendedor")
            const Vendedor = d.getElementById("Vendedor")
            const precios = d.querySelectorAll(".precios")
            const vendedores = d.querySelectorAll(".vendedores")

            for(i = 0; i < precios.length; i++){
                if(precios[i].value == valorPrecio.value){
                    TipoPrecio.removeChild(precios[i])
                }
            }
            for(i = 0; i < vendedores.length; i++){
                if(vendedores[i].textContent == valorVendedor.textContent){
                    Vendedor.removeChild(vendedores[i])
                }
            }
              $(document).ready(function () {
            $(".js-exam").select2();
        });
        d.addEventListener("keydown", e => {
            if(e.keyCode == 120 ){
                window.open('/facturacion/acceso-directo-stock', 'Stock', "width=1400, heigh=100%")
            }
        })

        </script>
