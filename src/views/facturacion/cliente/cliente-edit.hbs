    <main>

        <div class="container-fluid mt-4">
            <div class="row">
                <div class="col-md">

                    <div class="container-fluid mt-2">
                        {{#each errors}}

                        <div class="alert alert-danger alert-dismissible fade show" role="alert">
                            {{text}}
                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                        </div>

                        {{/each}}
                        {{#each ok}}

                        <div class="alert alert-success alert-dismissible fade show" role="alert">
                            {{text}}
                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                        </div>

                        {{/each}}
                        <div class="card">
                            <div class="card-header">
                                <h4 class="text-dark text-center bg-light">Editar cliente</h4>
                            </div>
                            <form action="/facturacion/cliente-edited/{{cliente._id}}?_method=PUT" class="form-group"
                                method="POST">
                                <input type="hidden" name="_method" value="PUT">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-sm-3">
                                            <label for="" class="text-dark">Nombres:</label>
                                            <input type="text" name="Nombres"  value="{{cliente.Nombres}}" class="form-control"
                                                placeholder="Introduzca los nombres del cliente">
                                        </div>
                                        <div class="col-sm-3">
                                            <label for="" class="text-dark">Apellidos:</label>
                                            <input type="text" name="Apellidos" value="{{cliente.Apellidos}}" class="form-control"
                                                placeholder="Introduzca los apellidos del cliente">
                                        </div>
                                        <div class="col-sm-3">
                                            <label for="" class="text-dark">Cedula</label>
                                            <input type="number" name="Cedula" value="{{cliente.Cedula}}" 
                                                placeholder="Introduzca la cedula del cliente" class="form-control">
                                        </div>
                                        <div class="col-sm-3">
                                            <label for="" class="text-dark">Empresa o negocio</label>
                                            <input type="text" name="Empresa" value="{{cliente.Empresa}}"
                                                placeholder="Introduzca el nombre del negocio" class="form-control">
                                        </div>
                                        <div class="col-sm-8">
                                            <label for="" class="text-dark">Dirección fiscal:</label>
                                            <input type="text" name="Direccion" value="{{cliente.Direccion}}" 
                                                placeholder="Introduzca la dirección fiscal" class="form-control">
                                        </div>
                                        <div class="col-sm-4">
                                            <label for="" class="text-dark">RIF:</label>
                                            <input type="text" name="Rif" class="form-control"
                                                placeholder="introduzca RIF del negocio o empresa" value="{{cliente.Rif}}">
                                        </div>
                                        <div class="col-sm-6">
                                            <label for="" class="text-dark">Numero de telefono</label>
                                            <input type="number" name="Telefono"
                                                placeholder="Introfuzca numero de telefono" value="{{cliente.Telefono}}"  class="form-control">
                                        </div>
                                        <div class="col-sm-6">
                                            <label for="" class="text-dark">Numero de celular</label>
                                            <input type="number" name="Celular"
                                                placeholder="Introfuzca numero de celular" value="{{cliente.Celular}}"  class="form-control">
                                        </div>
                                        <div class="col-sm-6">
                                            <label for="" class="text-dark">Tipo precio</label>
                                            <select name="TipoPrecio" class="form-control" id="TipoPrecio">
                                                <option value="{{cliente.TipoPrecio}}" id="valorPrecio">PRECIO {{cliente.TipoPrecio}}</option>
                                                <option value="GRANMAYOR" class="precios">PRECIO GRAN MAYOR</option>
                                                <option value="MAYOR" class="precios">PRECIO MAYOR</option>
                                                <option value="DETAL" class="precios">PRECIO DETAL</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-6">
                                            <label class="text-dark">Vendedor asignado</label>
                                            <select name="Vendedor" class="form-control js-exam" id="Vendedor">
                                                <option value="{{cliente.Vendedor}}" id="valorVendedor">{{cliente.Vendedor}}</option>
                                                {{#each vendedor}}
                                                <option value="{{Username}}" class="vendedores text-dark">{{Username}}</option>
                                                {{/each}}
                                            </select>
                                        </div>
                                    </div>
                                </div>
                        </div>
                        <div class="mt-2 mb-3 text-end">
                            <input type="submit" value="Actualizar cliente" class="btn btn-outline-success">
                        </div>
                        </form>
                         <div class="col-md text-end mt-4 mr-4">

                            <button type="button" class="btn btn-outline-danger" data-toggle="modal"
                                data-target="#exampleModal">
                                Eliminar cliente
                            </button>
                        </div>
                    </div>

                </div>
            </div>
        </div>
                    </div>
                </div>
            </div>
            <!-- Modal -->
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                aria-hidden="true">
                <div class="modal-dialog " role="document">
                    <div class="modal-content bg-dark">
                        <div class="modal-header">
                            <h5 class="modal-title text-center" id="exampleModalLabel">Eliminar Cliente</h5>
                            <button type="button" class="close btn-secondary h4" data-dismiss="modal"
                                aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <p class="h5">
                                ¿Estas seguro/a que quieres eliminar al cliente <strong class="text-light">
                                    {{cliente.Empresa}}</strong>?
                            </p>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                            <form action="/facturacion/cliente-delete/{{cliente._id}}?_method=DELETE" method="POST">
                                <input type="hidden" name="_method" value="DELETE">
                                <button type="submit" class="btn btn-danger">Eliminar</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </main>

